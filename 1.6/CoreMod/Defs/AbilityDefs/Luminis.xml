<?xml version="1.0" encoding="UTF-8"?>
<Defs>
    
	
	<!-- Level 1 -->
    
	<ThingDef ParentName="EtherealThingBase">
    <defName>VPE_GreaterSolarPinholeSTNL</defName>
    <label>Greater Solar Pinhole</label>
    <thingClass>ThingWithComps</thingClass>
    <tickerType>Normal</tickerType>
    <drawerType>RealtimeOnly</drawerType>
    <drawOffscreen>true</drawOffscreen>
    <category>PsychicEmitter</category>
    <altitudeLayer>MoteOverheadLow</altitudeLayer>
	<selectable>true</selectable>
	    <building>
      <isInert>true</isInert>
      <claimable>true</claimable>
      <deconstructible>false</deconstructible>
    </building>
    <graphicData>
      <graphicClass>Graphic_Single</graphicClass>
      <texPath>Things/Mote/SolarPinhole</texPath>
      <shaderType>MoteGlowDistorted</shaderType>
      <color>(255, 255, 245)</color>
      <drawSize>6</drawSize>
      <shaderParameters>
        <_DistortionTex>/Other/Ripples</_DistortionTex>
        <_distortionIntensity>0.05</_distortionIntensity>
        <_DistortionScale>0.4</_DistortionScale>
        <_distortionScrollSpeed>(0.07, 0.07, 0.0)</_distortionScrollSpeed>
      </shaderParameters>
    </graphicData>
    <comps>
      <li Class="CompProperties_Glower">
        <glowRadius>30</glowRadius>
		<overlightRadius>18</overlightRadius>
        <glowColor>(217,217,208,0)</glowColor>
      </li>
      <li Class="CompProperties_HeatPusher">
        <compClass>CompHeatPusherPowered</compClass>
        <heatPerSecond>20</heatPerSecond>
        <heatPushMaxTemperature>60</heatPushMaxTemperature>
      </li>
	  <li Class="VPE_Luminis.CompProperties_DestroyGizmo"/>
    </comps>
  </ThingDef>
  
  <VEF.Abilities.AbilityDef ParentName="VPE_PsycastBase">
    <defName>VPE_GreaterSolarPinholeSTNL</defName>
    <label>Greater Solar Pinhole</label>
    <description>Form a miniature portal directly linked to the nearest star, this pinhole will allow more light to pass through, allowing for plant growth. This portal is large enough to persist and will remain until manually dispersed at the cost of letting out a lot more heat than a standard solar pinhole.</description>
    <iconPath>UI_LuminisSTNL/Abilities/Luminis/GreaterSolarPinhole</iconPath>
    <abilityClass>VEF.Abilities.Ability_Spawn</abilityClass>
    <needsTicking>true</needsTicking>
    <castTime>15</castTime>
    <durationTime>30000</durationTime>
    <radius>29.9</radius>
    <range>24.9</range>
    <targetMode>Location</targetMode>
    <showUndrafted>true</showUndrafted>
    <chance>0</chance>
    <modExtensions>
      <li Class="VanillaPsycastsExpanded.AbilityExtension_Psycast">
        <path>VPE_LuminisSTNL</path>
        <level>1</level>
        <order>1</order>
        <psyfocusCost>0.25</psyfocusCost>
        <entropyGain>0</entropyGain>
      </li>
      <li Class="VEF.Abilities.AbilityExtension_Spawn">
        <thing>VPE_GreaterSolarPinholeSTNL</thing>
      </li>
      <li Class="VEF.Abilities.AbilityExtension_EffecterOnTarget">
        <effecterDef>Skip_Exit</effecterDef>
        <maintainForTicks>60</maintainForTicks>
        <scale>0.42</scale>
      </li>
      <li Class="VEF.Abilities.AbilityExtension_FleckOnTarget">
        <fleckDefs>
          <li>PsycastSkipInnerExit</li>
          <li>PsycastSkipOuterRingExit</li>
        </fleckDefs>
        <scale>0.6</scale>
        <sound>Psycast_Skip_Exit</sound>
        <preCastTicks>5</preCastTicks>
      </li>
    </modExtensions>
  </VEF.Abilities.AbilityDef>
  
   <!-- Level 2 -->
   
  <VEF.Abilities.AbilityDef ParentName="VPE_PsycastBase">
    <defName>VPE_BlindingFlashSTNL</defName>
    <label>Blinding Flash</label>
    <description>Generate a bright explosion of light that will temporarily blind and disorient the targets, stunning them for a few seconds as their vision returns to normal. \n\nDuration of the stun scales with psychic sensitivity.</description>
    <iconPath>UI_LuminisSTNL/Abilities/Luminis/BlindingFlash</iconPath>
    <abilityClass>VPE_Luminis.Ability_BlindingFlash</abilityClass>
    <needsTicking>true</needsTicking>
    <castTime>30</castTime>
	<castSound>PsycastPsychicPulse</castSound>
    <durationTime>120</durationTime>
	<durationTimeStatFactors>
	<PsychicSensitivity>1</PsychicSensitivity>
	</durationTimeStatFactors>
    <radius>5.9</radius>
    <range>24.9</range>
    <targetMode>Location</targetMode>
    <showUndrafted>false</showUndrafted>
    <chance>1</chance>
    <modExtensions>
      <li Class="VanillaPsycastsExpanded.AbilityExtension_Psycast">
        <path>VPE_LuminisSTNL</path>
        <level>2</level>
        <order>2</order>
        <psyfocusCost>0.04</psyfocusCost>
        <entropyGain>10</entropyGain>
		<prerequisites>
		<li>VPE_GreaterSolarPinholeSTNL</li>
		</prerequisites>
      </li>
    </modExtensions>
  </VEF.Abilities.AbilityDef>
  

  
